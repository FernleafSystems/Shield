<select class="selectpicker" data-live-search="true"
		title="{{ strings.jump_to_module }}"
		data-size="10"
		onchange="location = this.value;">
	{% for mod in vars.mods %}
		{% if mod.show_mod_opts %}
			<option value="{{ mod.href }}">
				{{ mod.name }}
				{% if vars.active_module_settings|default('') == mod.slug %}({{ strings.this_page|lower }}){% endif %}
			</option>
		{% endif %}
	{% endfor %}
</select>