
{% if flags.has_notes %}
	<table class="table table-hover mb-0">
		<thead><tr>
			<th>Note</th>
			<th>User</th>
			<th>Date</th>
		</tr></thead>
		{% for note in vars.insight_notes %}
			<tr class="message_row">
				<td>{{ note.note }}</td>
				<td>{{ note.wp_username }}</td>
				<td>{{ note.created_at }}</td>
			</tr>
		{% endfor %}
	</table>
{% else %}
	<div class="alert alert-info">There are no admin notes.</div>
{% endif %}