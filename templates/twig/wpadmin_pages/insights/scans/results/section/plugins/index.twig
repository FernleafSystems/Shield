<div class="row mt-3" id="ScanResultsPlugins">

	<div class="col-2">
		<ul class="nav nav-tabs flex-column">
			{% for plugin_key,plugin in vars.plugins %}
				<li class="nav-item mb-2">
					<a class="nav-link {% if plugin_key < 1 %}active{% endif %}"
					   href="#plugin-tab-{{ plugin.slug }}"
					   data-toggle="tab"
					>{{ plugin.name }}</a>
				</li>
			{% endfor %}
		</ul>
	</div>

	<div class="col-10">
		<div class="tab-content">
			{% for plugin_key,plugin in vars.plugins %}
				<div class="tab-pane fade {% if plugin_key < 1 %}show active{% endif %}"
					 id="plugin-tab-{{ plugin.slug }}"
					 role="tabpanel"
					 aria-labelledby="profile-tab"
				>
					{% include '/wpadmin_pages/insights/scans/results/section/plugins/plugin_panel.twig' %}
				</div>
			{% endfor %}
		</div>
	</div>
</div>