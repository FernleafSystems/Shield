<div class="row insights_widget" id="SectionScanResultsWcf">
	<div class="col-12">
		<div class="card card-scan_results mt-4">
			
			<div class="card-header">
				<h5 class="card-title">WordPress Core File Integrity [ {{ scan.count }} ]</h5>
				<h6 class="card-subtitle mb-2 text-muted">
					Files are either missing or have been modified in some way
					when compared with the official distribution from WordPress.org
				</h6>
			</div>

			<div class="card-body">
				<div id="TableScanWcf"></div>
			</div>

			<div class="card-footer">
				<div class="row">
					<div class="col-sm-6">{{ scan.last_scan_at }}</div>
					<div class="col-sm-6 text-right">
						<a href="{{ scan.hrefs.options }}"
						   class="btn btn-sm btn-outline-dark">{{ strings.options }} &rarr;</a>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>
<script>
	var $oTableScanWcf = jQuery( '#TableScanWcf' ).icwpWpsfAjaxTable(
		{
			'ajax_render':{{ ajax.render_table_scan|raw }},
			'req_params': {
				'fScan': 'wcf'
			}
		}
	);
	jQuery( '#SectionScanResultsWcf' ).icwpWpsfScanResults(
		{
			'table': $oTableScanWcf,
			'ajax_item_delete':{{ ajax.item_delete|raw }},
			'ajax_item_ignore':{{ ajax.item_ignore|raw }},
			'ajax_item_repair':{{ ajax.item_repair|raw }},
			'req_params': {
				'fScan': 'wcf'
			}
		}
	);
</script>