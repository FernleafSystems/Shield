<div class="row p-2" id="SectionStats">
	{% for stat in vars.insight_stats %}
		<div class="col-lg-2">
			<div class="stat card text-center p-0 m-0"
					{% if stat.tooltip is defined %}
						title="{{ stat.tooltip }}"
					{% endif %}
			>
				<div class="card-header">
					{{ stat.title }}
				</div>
				<div class="card-body">
					{{ stat.val }}
				</div>
			</div>
		</div>
	{% endfor %}
</div>


